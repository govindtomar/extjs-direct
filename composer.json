{
    "name": "govindtomar/extjs-direct",
    "type": "library",
    "description": "A base component to integrate Sencha Ext JS Ext.direct into a PHP application",
    "homepage": "https://github.com/govindtomar/extjs-direct",
    "license": "MIT",
    "authors": [
        {
            "name": "Govind Tomar",
            "email": "govindtomar01@gmail.com",
            "homepage": "https://www.govindtomar.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "GT\\ExtDirect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GT\\ExtDirect\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "8.0.*|8.1.*",
        "ext-json": "*",
        "doctrine/annotations": "~3.0.1",
        "jms/metadata": "~2.6",
        "jms/serializer": "~3.17",
        "symfony/dependency-injection": "~6.4.8",
        "symfony/event-dispatcher": "~6.4.8",
        "symfony/expression-language": "~6.4.8",
        "symfony/http-foundation": "~6.4.8",
        "symfony/security-core": "~6.4.8",
        "symfony/validator": "~6.4.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "~6.4.8",
        "symfony/stopwatch": "~6.4.8"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
